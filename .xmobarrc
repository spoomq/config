Config { bgColor = "black"
  , fgColor = "blue"
  , position = Top
  , border = NoBorder
  , borderColor = "blue"
  , lowerOnStart = True
  , commands = [ Run Date "%a %b %_d %Y %H:%M" "date" 10
               , Run StdinReader
               ]
  , sepChar = "%"
  , alignSep = "}{"
  , template = "%StdinReader%" }{ <fc=#ee9100>%date%</fc>
}
